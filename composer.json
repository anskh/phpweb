{
    "name": "anskh/phpweb",
    "description": "Simple PHP Website Foundation",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "nikic/fast-route": "^1.3",
        "laminas/laminas-diactoros": "^2.8",
        "laminas/laminas-httphandlerrunner": "^2.1",
        "woohoolabs/harmony": "^7.1",
        "slim/php-view": "^3.1",
        "monolog/monolog": "^2.3"
    },
    "require-dev": {
        "filp/whoops": "^2.14"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Anskh\\PhpWeb\\": "src/",
            "Anskh\\Example\\": "example/"
        },
        "files": ["src/functions.php"]
    },
    "authors": [{
        "name": "Khaerul Anas",
        "email": "anasikova@gmail.com"
    }],
    "minimum-stability": "stable",
    "scripts": {
        "post-update-cmd": ["composer dump-autoload"],
        "db:migrate": ["php -f example/migrations.php"]
    }
}